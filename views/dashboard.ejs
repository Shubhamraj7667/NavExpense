                          <!-- <td><%-eachData.transaction_date%></td> -->
                          <td><%-eachData.amount%></td>
                          


                          <td>
                            <span
                              class="badge <%= eachData.transaction_type === 'credit' ? 'bg-success' : 'bg-danger' %>">
                              <%- eachData.transaction_type %>

                            </span>
                          </td>
                          <td><%- eachData.comment%></td>
                          <td>
                            <button class="btn btn-danger" id = "delete_transaction">Delete</i></button>
                            <button class="btn btn-success edit_transaction">Edit</button>
                          </td>
                        </tr>
                        
                        <% }) %>

                    </tbody>
                  </table>



