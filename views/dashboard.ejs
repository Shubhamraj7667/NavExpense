                  <!-- Line Chart -->
                  <div id="reportsChart"></div>

                  <script>
                    document.addEventListener("DOMContentLoaded", () => {
                      const totalDebitGraph = <%= JSON.stringify(totalDebitGraph) %>;
                      const totalCreditGraph = <%= JSON.stringify(totalCreditGraph) %>;
                      // const dates = <%= JSON.stringify(new Date(dates).toLocaleString('en-US', { year: 'numeric', month: 'long', day: 'numeric' })) %>;
                      const totalSevingGraph = <%= JSON.stringify(totalSevingGraph) %>;

                      const encodedDates = " <%=  JSON.stringify(dates) %>";
                      const decodedString = encodedDates.replace(/&#34;/g, '"');
                      const dates = JSON.parse(`${decodedString}`);
                      // const dates = <%= JSON.stringify(dates) %>;

                      console.log(dates);

                      new ApexCharts(document.querySelector("#reportsChart"), {
                        series: [{
                          name: 'Debit',
                          data: totalDebitGraph,
                        }, {
                          name: 'Credit',
                          data: totalCreditGraph
                        }, {
                          name: 'Savings',
                          data: totalSevingGraph
                        }],
                        chart: {
                          height: 350,
                          type: 'area',
                          toolbar: {
                            show: false
                          },
                        },
                        markers: {
                          size: 4
                        },
                        colors: ['#4154f1', '#2eca6a', '#ff771d'],
                        fill: {
                          type: "gradient",
                          gradient: {
                            shadeIntensity: 1,
                            opacityFrom: 0.3,
                            opacityTo: 0.4,
                            stops: [0, 90, 100]
                          }
                        },
                        dataLabels: {
                          enabled: false
                        },
                        stroke: {
                          curve: 'smooth',
                          width: 2
                        },
                        xaxis: {
                          type: 'date',
                          categories: dates
                        },
                        tooltip: {
                          x: {
                            format: 'dd/MM/yy'
                          },
                        }
                      }).render();
                    });
                  </script>
                  <!-- End Line Chart -->

                </div>

              </div>
